<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
    <worksheet name="Sheet2">
        <section startRow="0" endRow="0"/>
            <loop startRow="1" endRow="1" items="items" var="department" varType="model.Department">
               <section startRow="1" endRow="1">
                   <mapping row="1" col="0">department.name</mapping>
                   <mapping row="1" col="1">department.age</mapping>
                   <mapping row="1" col="2">department.payment</mapping>
                   <mapping row="1" col="3">department.bonus</mapping>
               </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
    </workbook>